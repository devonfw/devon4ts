{
  "$schema": "http://json-schema.org/schema",
  "$id": "InitTypeormSchematic",
  "title": "Init Typeorm Schema",
  "type": "object",
  "properties": {
    "db": {
      "type": "string",
      "enum": [
        "postgres",
        "cockroachdb",
        "mariadb",
        "mysql",
        "sqlite",
        "oracle",
        "mssql",
        "mongodb"
      ],
      "x-prompt": "What kind of database do you want to use?"
    }
  },
  "required": [
    "db"
  ]
}
