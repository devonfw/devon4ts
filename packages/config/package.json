{
  "name": "@devon4node/config",
  "version": "2.0.0",
  "description": "devon4node module for easy application configuration.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint {src,apps,lib,test}/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "devon4node",
    "devonfw"
  ],
  "author": {
    "name": "Capgemini",
    "url": "https://www.capgemini.com/"
  },
  "contributors": [
    "Dario Rodriguez Gonzalez <dario.rodriguez-gonzalez@capgemini.com>"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "@nestjs/common": "^7.0.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "lodash": "^4.17.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devonfw/devon4node.git"
  },
  "bugs": {
    "url": "https://github.com/devonfw/devon4node/issues"
  },
  "homepage": "https://github.com/devonfw/devon4node",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
